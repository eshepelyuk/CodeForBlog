<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date{ISO8601} %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="ua.eshepelyuk">
        <appender-ref ref="STDOUT"/>
    </logger>

</configuration>